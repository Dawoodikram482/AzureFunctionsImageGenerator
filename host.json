{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 8,
      "maxDequeueCount": 5,
      "newBatchThreshold": 4,
      "maxPollingInterval": "00:00:02"
    }
  }
}
